<h2>Users in the bank</h2>
<% users.forEach(user => { %>
<div><%= user.first_name + ' ' + user.last_name %></div>
<a href="/users/<%= user.personal_number %>">View</a>
<a href="/users/<%= user.personal_number %>/edit">Edit</a>
<%- include('../partials/deleteForm', { url: `/users/${user.personal_number}` })
%> <% }) %>
