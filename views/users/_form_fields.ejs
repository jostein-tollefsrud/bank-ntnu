<!-- First name -->
<div>
    <label for="first_name">First Name</label>
    <input
        type="text"
        name="first_name"
        id="first_name"
        value="<%= user.first_name %>"
    />
</div>

<!-- Last name -->
<div>
    <label for="last_name">Last Name</label>
    <input
        type="text"
        name="last_name"
        id="last_name"
        value="<%= user.last_name %>"
    />
</div>

<!-- Date of birth -->
<div>
    <label for="date_of_birth">Date of birth</label>
    <input
        type="date"
        name="date_of_birth"
        id="date_of_birth"
        value="<%= user.date_of_birth == null ? '' : user.date_of_birth.toISOString().split('T')[0] %>"
    />
</div>

<!-- City -->
<div>
    <label for="city">City</label>
    <input type="text" name="city" id="city" value="<%= user.city %>" />
</div>

<!-- Personal number -->
<div>
    <label for="personal_number">Personal number</label>
    <input
        type="text"
        name="personal_number"
        id="personal_number"
        value="<%= user.personal_number %>"
    />
</div>
